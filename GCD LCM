#include<bits/stdc++.h>
typedef long long ll;
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);

   int t;
   cin >> t;
   while(t--)
   {
       int n, m;
       cin >> n >> m;

       if(m % n != 0)   cout << -1 << "\n";
       else cout << n << " " << m << "\n";




   }



    return 0;
}
