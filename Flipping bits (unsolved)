#include <bits/stdc++.h>
typedef long long ll;
using namespace std;



int main()
{
    int t;
    cin >> t;

    while(t--)
    {
        int n;
        cin >> n;

        string num = bitset<32>((-n)).to_string();



        cout << bitset<32>(num).to_ulong() - 1 << "\n";

    }
         return 0;
}
