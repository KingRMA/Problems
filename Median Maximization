#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin >> t;
    while(t--)
    {
        int n, m;
        cin >> n >> m;

        if (n == 1) 
        {
            cout << m << "\n";
            continue;
        }
        else
        {
            int pos = ceil((double)(n) / (2.0));
            pos--;
            int res = m / (n - pos);
            cout << res << "\n";
            continue;
        }
        
    }

}
