#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;

    for (int i = 0; i < t; i++) 
    {
        ll s1, e1, s2, e2;
        cin >> s1 >> e1 >> s2 >> e2;

        ll l = max(s1, s2);
        ll r = min(e1, e2);

        if (l < r) cout << r - l << "\n";
        else cout << "0\n";
    }

    return 0;
}
