#include <bits/stdc++.h>
typedef long long ll;
using namespace std;



int main()
{

    int t;
    cin >> t;
    while(t--)
    {
        ll n;
        cin >> n;

        int m = 1, c = 0;
        while (n > 0)
        {
            if ((n & 1) == 0)   c |= m;
            m <<= 1;
            n >>= 1;
        }

        cout << c << "\n";

    }










    return 0;
}
