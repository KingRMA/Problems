#include <bits/stdc++.h>
typedef long long ll;
using namespace std;

map<pair<int, int>, string> mp;


int main() 
{
    int t;
    cin >> t;

    while(t--)
    {
        int a, b;
        string s;

        cin >> a >> b >> s;

        mp[{a, b}] = s;
    }

    cin >> t;

    while(t--)
    {
        int temp1, temp2;
        cin >> temp1 >> temp2;
        cout << mp[{temp1, temp2}] << "\n";
    }


    return 0;
}
